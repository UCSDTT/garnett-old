doctype html
html(ng-app="garnett")
    title #{title}
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    base(href="/")
    link(rel='shortcut icon' type='image/png' href='/images/garnett-favicon.png')
    link(href='/libs/bootstrap/dist/css/bootstrap.css', rel='stylesheet')
    link(href='/libs/bootstrap/dist/css/bootstrap-theme.css', rel='stylesheet')
    link(href='/css/signin.css', rel='stylesheet')
    link(href='/css/dashboard.css', rel='stylesheet')
  body
    block body
    block javascripts
      script(src='/libs/jquery/dist/jquery.js')
      script(src='/libs/bootstrap/dist/js/bootstrap.js')
      // script(src='/libs/angular/angular.js')
      script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.13/angular.js")
      script(src='/libs/angular-route/angular-route.js')
      // script(src='/libs/angular-bootstrap/ui-bootstrap.js')
      script(src='/libs/lodash/dist/lodash.min.js')
      script(src='https://maps.googleapis.com/maps/api/js?libraries=places&sensor=false')
      script(src='/libs/angular-google-maps/dist/angular-google-maps.min.js')
      script(src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.13.0.js")
      script(src='/js/app.js')
      script(src='/js/services/sampleService.js')
      script(src='/js/controllers/sampleController.js')
      script(src='/js/controllers/timelineController.js')
      script(src='/js/controllers/newEventController.js')
      script(src='/js/filters/sampleFilter.js')
      script(src='/js/directives/sampleDirective.js')
      script(src='/js/directives/ngAutoComplete.js')
